  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "yarn build && npx thirdweb@latest upload dist",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
